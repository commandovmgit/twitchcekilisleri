{
    "name": "Twitch Çekilişleri",
    "description": "Twitch yayıncıları için harikulade bir çekiliş oluşturma aracı.",
    "version": "2.5",
    "manifest_version": 2,
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "permissions": [
        "storage",
        "*://*.twitch.tv/*",
        "https://twitch.tv/*",
        "*://*.jtvnw.net/*",
        "http://www.w3.org/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval';object-src 'self';",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.twitch.tv/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "key": "",
    "update_url": "https://raw.githubusercontent.com/enharukalo/twitchcekilisleri/master/src/updates.xml",
    "web_accessible_resources": [
        "main.html",
        "send-message.js"
    ]
}